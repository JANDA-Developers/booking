    "babel-cli": "^6.26.0",
    "classnames": "^2.2.6",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-graphql": "^3.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react-hooks": "^1.0.1",
    "global": "^4.3.2",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.10.0",
    "lodash.debounce": "^4.0.8",
    "node-sass": "^4.11.0",
    "npm": "^6.7.0",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-apollo": "^2.3.3",
    "react-day-picker": "^7.2.4",
    "react-dom": "^16.7.0",
    "react-modal": "^3.8.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.1",
    "react-search-field": "^1.0.0",
    "react-select": "^2.3.0",
    "react-tooltip": "^3.9.2",
    "stylelint": "^9.10.1",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-recommended-scss": "^3.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "parser": "babel-eslint",
    "extends": "airbnb",
    "plugins": [
      "react",
      "jsx-a11y",
      "import",
      "graphql",
      "prettier",
      "react-hooks"
    ],
    "rules": {
      "react/jsx-filename-extension": 0,
      "linebreak-style": 0,
      "react-hooks/rules-of-hooks": "error",
      "no-console": "off",
      "//": [
        "아래는 label은 input을 자식으로 포함해야한다 를 제거"
      ],
      "jsx-a11y/label-has-for": [
        2,
        {
          "components": [
            "Label"
          ],
          "required": {
            "every": [
              "nesting",
              "id"
            ]
          },
          "allowChildren": true
        }
      ]
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
