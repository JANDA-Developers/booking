  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "parser": "babel-eslint",
    "extends": "airbnb",
    "plugins": [
      "react",
      "jsx-a11y",
      "import",
      "graphql",
      "prettier",
      "react-hooks"
    ],
    "rules": {
      "react/jsx-filename-extension": 0,
      "linebreak-style": 0,
      "react/forbid-prop-types": 0,
      "react-hooks/rules-of-hooks": "error",
      "no-unused-expressions": 0,
      "no-underscore-dangle": 0,
      "no-console": "off",
      "import/no-unresolved": "off",
      "max-len": [
        1,
        120,
        2
      ],
      "//": [
        "아래는 label은 input을 자식으로 포함해야한다 를 제거"
      ],
      "jsx-a11y/label-has-for": [
        2,
        {
          "components": [
            "Label"
          ],
          "required": {
            "every": [
              "nesting",
              "id"
            ]
          },
          "allowChildren": true
        }
      ]
    }
  },
  "eslintIgnore": [
    "naverMap.js",
    "**/*.js"
  ],
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@types/faker": "^4.1.5",
    "@types/googlemaps": "^3.30.19",
    "@types/graphql": "^14.0.7",
    "@types/jquery": "^3.3.29",
    "@types/moment": "^2.13.0",
    "@types/react": "^16.8.6",
    "@types/react-dom": "^16.8.2",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-graphql": "^3.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react-hooks": "^1.0.1",
    "stylelint": "^9.10.1",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-recommended-scss": "^3.2.0",
    "typescript": "^3.3.4000"
  }
}
